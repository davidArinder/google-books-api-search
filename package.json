{
  "name": "express-challenge",
  "version": "1.0.0",
  "description": "A small coding challenge to implement a full-stack integration with the Google Books API",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.20.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mongodb": "^3.6.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "server": "nodemon api/server.js",
    "browser": "cd browser && npm run start",
    "start": "concurrently -n 'server,client' -c 'blue,green' \"npm run server\" \"npm run browser\"",
    "postinstall": "cd browser && npm i"
  },
